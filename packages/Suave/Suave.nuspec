        <dependency id="FSharp.Core" version="[4.0.0,5.0.0)" />
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="FSharp.Core" version="[4.0.0,5.0.0)" />
      </group>
    </dependencies>
  </metadata>
</package>